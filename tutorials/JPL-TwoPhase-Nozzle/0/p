FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [1 -1 -2 0 0 0 0];

internalField       uniform 101325;

boundaryField
{
    inlet
    {
        type               calculated;
        value              $internalField;
    }

    "(outlet|outlet-front|outlet-top)"
    {
        type               calculated;
        value              $internalField;
    }

    Wall
    {
        type               calculated;
        value              $internalField;
    }

    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    "(PCWall|nozzleWall|nozzleAtmWall|atmWall)"
    {
        $Wall;
    }
}


// ************************************************************************* //
