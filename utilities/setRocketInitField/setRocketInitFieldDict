FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setRocketInitFieldDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Rocket Motor Parameters
chamberLength   0.0762;
boreRadius      0.003175;

gasField            "alpha.gas";
propellantField     "alpha.propellant";
type           cylinder;
direction      (0 0 1);
radius         $boreRadius;
origin         (0 0 0);

regions
(
    boxToCell
    {
        box (0 -1 $chamberLength) (1 1 1);
        fieldValues
        (
            volScalarFieldValue alpha.gas 1.0
            volScalarFieldValue alpha.propellant 0.0
        );
    }
);

// ************************************************************************* //
